{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "8.4.*",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "beberlei/assert": "v3.x-dev",
        "moneyphp/money": "^4.7.1",
        "nesbot/carbon": "^3.10.1",
        "phpoffice/phpspreadsheet": "^4.5.0",
        "symfony/console": "^7.4",
        "symfony/dotenv": "^7.4",
        "symfony/flex": "^2.4",
        "symfony/framework-bundle": "^7.4",
        "symfony/runtime": "^7.4",
        "symfony/yaml": "^7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.2.9",
        "symfony/browser-kit": "^7.4",
        "symfony/css-selector": "^7.4",
        "symfony/maker-bundle": "^1.59",
        "symfony/phpunit-bridge": "^7.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "test": "phpunit",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "^7.4"
        }
    }
}
